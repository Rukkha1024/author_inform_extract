{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "excel": {
          "command": "npx",
          "args": ["--yes", "@negokaz/excel-mcp-server"],
          "env": {
              "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
          }
    }
}
}